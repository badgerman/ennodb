#!/bin/sh

ADDRESS=$ITEST_HOST:$ITEST_PORT

curl -s --data '<img src="http://placekitten.com/g/240/300/" />' http://$ADDRESS/ennodb/cat
curl -s --data '<img src="http://placekitten.com/g/200/300/" />' http://$ADDRESS/ennodb/catz
output=`curl -s http://$ADDRESS/ennodb/cat`
if [ ! -n "$output" ]; then
  echo "ERROR: expected a value for 'cat'"
  exit 1
fi
